{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "ConnectionStrings": {
    //When empty a inmem database will be used
    "AwardData": ""
  },

  "SMTPOptions": {
    //Use smpt4dev or similar to send mail to localhost
    "Host": "localhost",
    "UserName": "",
    "PassWord": "",
    "From": "foo@bar.com"
  },

  "LetsEncrypt": {
    // Set this to automatically accept Let's Encrypt's terms of service.
    // If you don't set this in config, you will need to press "y" whenever the application starts
    "AcceptTermsOfService": true,

    // You must at least one domain name
    "DomainNames": [ "awardhacks.se", "www.awardhacks.se" ],

    // You must specify an email address to register with letsencrypt.org
    "EmailAddress": ""
  },

  "AppSettings": {
    //Send messages etc here
    "Email": "foo@bar.com",
    "Secret":  ""
  }
}